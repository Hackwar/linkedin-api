{
    "name": "joomla/linkedin",
    "type": "joomla-package",
    "description": "Joomla Linkedin Package",
    "keywords": ["joomla", "framework", "linkedin"],
    "homepage": "https://github.com/joomla-framework/linkedin-api",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
        "joomla/oauth1": "~1.0",
        "joomla/input": "~1.0",
        "joomla/http": "~1.0",
        "joomla/registry": "~1.0"
    },
    "require-dev": {
        "joomla/test": "~1.0",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
